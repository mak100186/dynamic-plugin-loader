{
  "appSettings": {
    "appName": "Plugin.Microsoft.Prototype",
    "plugins": [
      {
        "name": "PluginA.Main",
        "assemblyName": "PluginA.dll"
      },
      {
        "name": "PluginB.Main",
        "assemblyName": "PluginB.dll"
      }
    ]
  },
  "serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Async", "Serilog.Settings.Configuration", "Serilog.Enrichers.Environment" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.DataProtection": "Error",
        "Couchbase.Core": "Warning",
        "Couchbase.Query.QueryClient": "Warning",
        "Couchbase.Core.Diagnostics.Tracing": "Warning",
        "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "Fatal"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": { "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact" }
      }
    ],
    "Properties": {
      "AppName": "plugin-microsoft-prototype",
      "Release": "1.0-local"
    }
  }
}
